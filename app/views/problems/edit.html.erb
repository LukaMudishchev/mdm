<h1>Editing problem</h1>

<%  semantic_form_for(@problem) do |f| %>
    <%= render(:partial => "problem", :object => @problem, :locals => {:read_only => false, :f => f }) %>    
    <% f.buttons do %>
      <%= f.commit_button  %>
      <%= link_to 'Back', problems_path %>
    <% end %>
<% end %>


